{
  "name": "TPCFLU Flutter Dev",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "dart-code.dart-code",
        "dart-code.flutter"
      ]
    }
  },
  "postCreateCommand": "flutter doctor && flutter pub get",
  "remoteUser": "vscode",
  "forwardPorts": [5000],
  "features": {
    "ghcr.io/devcontainers/features/java:1": {},
    "ghcr.io/devcontainers/features/node:1": {}
  }
}