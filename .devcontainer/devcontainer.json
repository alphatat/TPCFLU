{
  "name": "Flutter Codespace",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "postCreateCommand": "flutter pub get",
  "customizations": {
    "vscode": {
      "extensions": ["dart-code.flutter", "dart-code.dart-code"]
    }
  },
  "forwardPorts": [8080],
  "remoteUser": "vscode"
}
